/*CLOSED UNION PLAZA PLANS*/
SELECT MON.YYYYMM_NR, PLAN.*
FROM AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_PLAN PLAN
INNER JOIN AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_MEET MEET
ON PLAN.MEET_ID = MEET.MEET_ID
INNER JOIN AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_MON MON
ON MEET.MON_ID = MON.MON_ID
WHERE MEET.PROV_ID = 4325
AND MEET.AUD_CURRENT_ROW_FLAG ='Y'
AND MON.AUD_CURRENT_ROW_FLAG = 'Y'
AND PLAN.AUD_CURRENT_ROW_FLAG ='Y'
AND PLAN.PLAN_STATUS_CD = 'C'

/*CLOSED GALLERIA PLANS*/
SELECT MON.YYYYMM_NR, PLAN.*
FROM AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_PLAN PLAN
INNER JOIN AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_MEET MEET
ON PLAN.MEET_ID = MEET.MEET_ID
INNER JOIN AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_MON MON
ON MEET.MON_ID = MON.MON_ID
WHERE MEET.PROV_ID = 5571
AND MEET.AUD_CURRENT_ROW_FLAG ='Y'
AND MON.AUD_CURRENT_ROW_FLAG = 'Y'
AND PLAN.AUD_CURRENT_ROW_FLAG ='Y'
--AND PLAN.PLAN_STATUS_CD = 'C'
--AND YYYYMM_NR >201800
AND MEAS_ID = 1345
order by yyyymm_nr
