SELECT FAC.FACILITY_NAME,FAC.FACILITY_NO, PROV.PROV_ID, PROV.PROV_NM, FAC.GROUP_NAME, DATA.MEAS_ID,MEAS.MEAS_NM, MON.YYYYMM_NR, DATA.DATA_VAL, DATA.BENCH_VAL
FROM AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_PROV_DATA DATA
	INNER JOIN AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_PROV PROV
		ON DATA.PROV_ID = PROV.PROV_ID
	INNER JOIN VIEW_DB.VIEW_DB_USR.FACILITY_DIM_VW FAC
		ON PROV.PROV_NM = FACILITY_NAME
	INNER JOIN AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_MEAS MEAS
		ON MEAS.MEAS_ID = DATA.MEAS_ID
	INNER JOIN AUD_FHR_QIFMM.AUD_FHR_QIFMM_USR.AUD_FHR_MON MON
		ON MON.MON_ID = DATA.MON_ID
WHERE DATA.AUD_CURRENT_ROW_FLAG = 'Y'
AND PROV.AUD_CURRENT_ROW_FLAG = 'Y'
AND MEAS.AUD_CURRENT_ROW_FLAG = 'Y'
AND MON.AUD_CURRENT_ROW_FLAG = 'Y'
AND DATA.MON_ID > 120
AND PROV.CURR_PARENT_FAC_ID = 0
/*AND DATA.MEAS_ID IN (1350,1211,1212)*/
--AND DATA.MEAS_ID = 1350
AND FAC.GROUP_NAME = 'Titan'
ORDER BY PROV.PROV_NM, DATA.MON_ID, DATA.MEAS_ID, MON.YYYYMM_NR
